    "@OA\\Info": {
        "prefix": "@OA\\Info",
        "body": [
            "/**",
            "* @OA\\Info(",
            "*     version=\"1.0\",",
            "*     title=\"${1:Title}\"",
            "* )",
            "*/",
        ]
    },

    "@OA\\PathItem": {
