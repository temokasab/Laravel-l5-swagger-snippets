            "*             mediaType=\"${1:application/json}\",",
            "*             @OA\\Schema(",
            "*                 schema=\"${2:Request}\",",
            "*                 title=\"${3:Title}\",",
            "*                 @OA\\Property(",
            "*                     property=\"${4:id}\",",
            "*                     type=\"${5:int}\"",
