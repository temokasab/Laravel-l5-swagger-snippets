            "*/",
        ]
    },

    "@OA\\Get": {
        "prefix": "@OA\\Get",
        "body": [
            "/**",
            "* @OA\\Get(",
            "*      path=\"${1:Path}\",",
            "*      operationId=\"${2:TestId}\",",
            "*      tags={\"${3:Tags}\"},",
            "*      summary=\"${4:Title}\",",
            "*      description=\"${5:Description}\",",
            "*      @OA\\Response(",
            "*          response=${7:Response Code},",
            "*          description=\"${8:Response Message}\",",
            "*       ),",
            "*     )",
            "*/"
        ]
    },

    "@OA\\Get With Param": {
